#include "hw.h"
#include <iostream>

using namespace std;

int main() {
	cout << "Power-up" << endl;
	platformInit();
	cout << "HW initialization done." << endl;
	int val = 0xA;
	cout << getResult() << endl;
	startTRNG();
	delayMs(100);
	cout << getResult() << endl;
	while(1) {
		setLed(val);
		val = ~val;
		delayMs(500);
	}
	return 0;
}
